{
  "name": "rest-api-fastify-boilerplate",
  "private": true,
  "version": "0.0.1",
  "description": "PoC",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "ts-node src/server.ts",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint --fix **/*.ts",
    "check:type": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "test:e2e": "jest --config ./e2e-test/jest.config.js --forceExit --passWithNoTests --coverage --reporters=jest-junit",
    "build": "rm -rf dist && tsc",
    "prepare": "husky install",
    "migrate:make": "knex migrate:make -x ts",
    "migrate:list": "knex migrate:list",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback "
  },
  "author": "Petr Grishin",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": ">=16.14.2",
    "npm": ">=8.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.4",
    "@types/node": "^20.8.7",
    "@types/node-jose": "^1.1.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^7.0.2",
    "jest": "^29.7.0",
    "jest-junit": "^13.0.0",
    "jose": "^4.6.1",
    "js-yaml": "^4.1.0",
    "nock": "^13.1.4",
    "node-jose": "^2.1.1",
    "node-mocks-http": "^1.11.0",
    "prettier": "^3.0.2",
    "semantic-release": "^21.0.9",
    "supertest": "^6.1.6",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/helmet": "^8.0.0",
    "@fastify/jwt": "^7.2.2",
    "@fastify/secure-session": "^4.0.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.10.1",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "@mgcrea/fastify-graceful-exit": "^1.1.1",
    "@mgcrea/pino-pretty-compact": "^1.3.0",
    "@sinclair/typebox": "^0.24.31",
    "config-decorator": "^1.1.0",
    "fastify": "^4.24.3",
    "fastify-decorators": "^3.15.1",
    "get-jwks": "^8.3.1",
    "json-schema-to-ts": "^2.9.2",
    "pino-pretty": "^7.6.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.3.0"
  }
}
